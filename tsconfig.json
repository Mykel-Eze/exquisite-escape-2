{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "allowImportingTsExtensions": true,
    "target": "esnext",
    "module": "esnext",
    "jsx": "preserve",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "lib": ["esnext", "dom"]
  },
  "rules": {
    "@typescript-eslint/no-unused-vars": "off"
  },
  "exclude": ["node_modules"]
}
